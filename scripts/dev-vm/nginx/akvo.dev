upstream rsr {
	server 33.33.33.15:80 fail_timeout=0;
    # server 127.0.0.1:1337 fail_timeout=0;
}

server {
 	listen 80;
 	server_name akvo.dev .akvoapp.dev;

  	access_log logs/akvo.dev.access.log;

	location / {
	      proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
	      proxy_set_header Host $http_host;
	      proxy_redirect off;

	      if (!-f $request_filename) {
	        proxy_pass http://rsr;
	        break;
	      }
	  }

}
